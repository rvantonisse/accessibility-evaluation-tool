{
  "name": "WCAG-EM-Report-Tool",
  "version": "3.0.3",
  "repository": {
    "type": "git",
    "url": "git://github.com/w3c/wcag-em-report-tool.git"
  },
  "type": "module",
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "vite build",
    "dev": "run-s clean:translations clean:dev vite:dev",
    "build": "run-s clean:translations clean:build vite:build",
    "preview": "vite preview",
    "build-production-rollup": "export BASEPATH='/WAI/eval/report-tool' && npm run clean:build && rollup -c",
    "build-ghpages-rollup": "export BASEPATH='/wai-wcag-em-report-tool' && npm run clean:build && rollup -c",
    "dev-rollup": "export BASEPATH='' && npm run clean:dev & rollup -c -w",
    "clean": "run-p clean:*",
    "clean:build": "rimraf ./build",
    "clean:dev": "rimraf ./build",
    "clean:node_modules": "rimraf ./node_modules",
    "clean:translations": "rimraf ./src/locales/translations_*.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "main": "src/main.js",
  "dependencies": {
    "jsonld": "^8.2.0",
    "marked": "^1.2.9",
    "sirv-cli": "^1.0.0",
    "svelte-navigator": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@storybook/addon-essentials": "^7.2.3",
    "@storybook/addon-interactions": "^7.2.3",
    "@storybook/addon-links": "^7.2.3",
    "@storybook/blocks": "^7.2.3",
    "@storybook/svelte": "^7.2.3",
    "@storybook/svelte-vite": "^7.2.3",
    "@storybook/testing-library": "^0.2.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "colors": "^1.4.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.15.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-svelte3": "^2.7.3",
    "globby": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "prettier-plugin-svelte": "^3.0.3",
    "rimraf": "^3.0.2",
    "storybook": "^7.2.3",
    "svelte": "^3.29.0",
    "svelte-i18n": "3.3.0",
    "vite": "^4.4.9",
    "vite-plugin-require-transform": "^1.0.21"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  }
}
